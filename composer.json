{
  "name": "kodepandai/laravel-api-response",
  "type": "library",
  "version": "1.1.0",
  "require": {
    "laravel/framework": "^8"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.2",
    "nunomaduro/larastan": "^1.0",
    "orchestra/testbench": "^6.23",
    "pestphp/pest": "^1.20"
  },
  "autoload": {
    "psr-4": {
      "KodePandai\\ApiResponse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KodePandai\\ApiResponse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "analyse": "phpstan analyse",
    "lint": "php-cs-fixer fix --allow-risky=yes",
    "pest": "pest --stop-on-failure",
    "test": [
      "@analyse",
      "@lint",
      "@pest"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "KodePandai\\ApiResponse\\ApiResponseServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
